@{
    Layout="Shared/_Layout.cshtml";
}
<div class="row">
  <div class="card blue lighten-5">
    <span class="card-title">Search Result</span>
    <div class="card-content blue-text accent-2">
      @if(@Model.Count== 0)
      {
        <h5> There is no match for your search</h5>
      }
      else
      {
        <ol>
        @foreach (var result in @Model)
        {
          <li><a href="/results/@result.GetId()"> @result.GetName()</a></li>
        }
        </ol>
      }
    </div>
  </div>
  <h5><a href="/">Home Page</a></h5>
  <h5><a href="/bands">See all bands</a></h5>
  <h5><a href="/venues">See all venues</a></h5>
</div>
